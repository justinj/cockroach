      └── json-agg [type=jsonb, outer=(5)]
           └── agg-distinct [type=jsonb, outer=(5)]
                └── variable: j [type=jsonb, outer=(5)]

norm
SELECT 1 FROM a AS ref_0 INNER JOIN a AS ref_1 ON EXISTS(SELECT 1 FROM a WHERE a.s = ref_0.s)
----
project
 ├── columns: "?column?":20(int!null)
 ├── fd: ()-->(20)
 ├── inner-join
 │    ├── columns: a.s:4(string)
 │    ├── semi-join
 │    │    ├── columns: a.s:4(string)
 │    │    ├── scan a
 │    │    │    └── columns: a.s:4(string)
 │    │    ├── scan a
 │    │    │    └── columns: a.s:16(string)
 │    │    └── filters [type=bool, outer=(4,16), constraints=(/4: (/NULL - ]; /16: (/NULL - ]), fd=(4)==(16), (16)==(4)]
